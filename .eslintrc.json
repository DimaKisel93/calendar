{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
       "react-app","prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12    
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}
